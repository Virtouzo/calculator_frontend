{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"jsx": "react",
		"strict": true,
		"lib": ["es2019", "DOM"],
		"noImplicitAny": false,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"typeRoots": ["./node_modules/@types"],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "."
	}
}
